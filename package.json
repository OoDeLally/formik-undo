    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.3",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "ts-jest": "^25.3.0",
    "typescript": "^3.8.3"
