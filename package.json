  "dependencies": {
    "formik": "^2.1.0",
    "react": "^16.8.0"
  },
  "devDependencies": {
    "rollup": "^2.9.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/classnames": "^2.2.5",
